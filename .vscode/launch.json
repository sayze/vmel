{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Vmel Debug",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceRoot}/bin/debug/vmel",
            "preLaunchTask": "build",
            "internalConsoleOptions": "openOnSessionStart",
            "logging": {
                "moduleLoad": false,
                "programOutput": true,
                "trace": false
            },
            "showDisplayString": false,
            "args": ["test-data/run.vml"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "osx": {
                "MIMode": "lldb"
            }
        }
    ]
}